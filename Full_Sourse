	i = 13849;
	c = 65537;
	cout << "Введите количество чисел" << endl;
	cin >> n;
	s = (rand() * 13 - 65) % 2;
	for (int k = 1; k <= n; k++)
	{
		s = (m * s + i) % c;
		cout << s << endl;
	}
	return 0;
}
