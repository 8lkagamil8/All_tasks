int main5() {
	cout << endl; cin.clear(); cin.ignore();
	int s;
	HWND hwnd = GetConsoleWindow();
	HDC hdc = GetDC(hwnd);

	system("cls");

	int x = 0;
	for (float i = 0; i < 3.14 * 1000; i += 0.05) {
		SetPixel(hdc, x, 50 + 25 * cos(i), RGB(255, 255, 255));
		x += 1;
	}

	ReleaseDC(hwnd, hdc);
	cin.clear(); cin.ignore();

	return 0;
}
int main6() {
	cout << endl; cin.clear(); cin.ignore();
	string s, w; int a; int res;
	res = 0;
	cout << "Введите строку: "; getline(cin, s);

	for (int i = 0; i <= s.size(); i++) {
		if ((s[i] != 'I') & (s[i] != 'i') & (s[i] != 'V') & (s[i] != 'v') & (s[i] != 'X') & (s[i] != 'x') & (s[i] != 'L') & (s[i] != 'l') & (s[i] != 'C') & (s[i] != 'c') & (s[i] != 'D') & (s[i] != 'd') & (s[i] != 'M') & (s[i] != 'm')) {
			s[i] = ' ';
		}
	}

	for (int i = 0; i <= s.size(); i++) {
		if (s[i] = ' ') {
			s.erase(i, 1);
		}
	}

	cout << s << endl;

	for (int t = 0; t <= s.size(); t++) {
		for (int z = 0; z < s.size(); z++) {
			if (s[z] == ' ') {
				swap(s[z], s[z + 1]);
			}
			else {
				if (s[z] == 'i') {
					s[z] = 'I';
				}
				else {
					if (s[z] == 'v') {
						s[z] = 'V';
					}
					else {
						if (s[z] == 'x') {
							s[z] = 'X';
						}
						else {
							if (s[z] == 'l') {
								s[z] = 'L';
							}
							else {
								if (s[z] == 'c') {
									s[z] = 'C';
								}
								else {
									if (s[z] == 'd') {
										s[z] = 'D';
									}
									else {
										if (s[z] == 'm') {
											s[z] = 'M';
										}
									}
								}
							}
						}
					}
				}
			}
		}
	}

	cout << s << endl;

	if (s[0] == 'I') res = 1;
	if (s[0] == 'V') res = 5;
	if (s[0] == 'X') res = 10;
	if (s[0] == 'L') res = 50;
	if (s[0] == 'C') res = 100;
	if (s[0] == 'D') res = 500;
	if (s[0] == 'M') res = 1000;

	for (int y = 0; y < s.size(); y++) {
		//------------------------------------------------------------Для единицы(I)
		if ((s[y] == 'I') & (s[y + 1] == 'I')) {
			res = res + 1;
		}
		else {
			if ((s[y] == 'I') & (s[y + 1] == 'V')) {
				res = 5 - res;
			}
			else {
				if ((s[y] == 'I') & (s[y + 1] == 'X')) {
					res = 10 - res;
				}
				else {
					if ((s[y] == 'I') & (s[y + 1] == 'L')) {
						res = 50 - res;
					}
					else {
						if ((s[y] == 'I') & (s[y + 1] == 'C')) {
							res = 100 - res;
						}
						else {
							if ((s[y] == 'I') & (s[y + 1] == 'D')) {
								res = 500 - res;
							}
							else {
								if ((s[y] == 'I') & (s[y + 1] == 'M')) {
									res = 1000 - res;
								}
							}
						}
					}
				}
			}
		}

		//------------------------------------------------------------Для пятерки(V)
		if ((s[y] == 'V') & (s[y + 1] == 'I')) {
			res = res + 1;
		}
		else {
			if ((s[y] == 'V') & (s[y + 1] == 'V')) {
				res = 5 + res;
			}
			else {
				if ((s[y] == 'V') & (s[y + 1] == 'X')) {
					res = 10 - res;
				}
				else {
					if ((s[y] == 'V') & (s[y + 1] == 'L')) {
						res = 50 - res;
					}
					else {
						if ((s[y] == 'V') & (s[y + 1] == 'C')) {
							res = 100 - res;
						}
						else {
							if ((s[y] == 'V') & (s[y + 1] == 'D')) {
								res = 500 - res;
							}
							else {
								if ((s[y] == 'V') & (s[y + 1] == 'M')) {
									res = 1000 - res;
								}
							}
						}
					}
				}
			}
		}

		//------------------------------------------------------------Для десяти(Х)
		if ((s[y] == 'X') & (s[y + 1] == 'I')) {
			res = res + 1;
		}
		else {
			if ((s[y] == 'X') & (s[y + 1] == 'V')) {
				res = 5 + res;
			}
			else {
				if ((s[y] == 'X') & (s[y + 1] == 'X')) {
					res = 10 + res;
				}
				else {
					if ((s[y] == 'X') & (s[y + 1] == 'L')) {
						res = 50 - res;
					}
					else {
						if ((s[y] == 'X') & (s[y + 1] == 'C')) {
							res = 100 - res;
						}
						else {
							if ((s[y] == 'X') & (s[y + 1] == 'D')) {
								res = 500 - res;
							}
							else {
								if ((s[y] == 'X') & (s[y + 1] == 'M')) {
									res = 1000 - res;
								}
							}
						}
					}
				}
			}
		}

		//------------------------------------------------------------Для пятидесяти(L)
		if ((s[y] == 'L') & (s[y + 1] == 'I')) {
			res = res + 1;
		}
		else {
			if ((s[y] == 'L') & (s[y + 1] == 'V')) {
				res = 5 + res;
			}
			else {
				if ((s[y] == 'L') & (s[y + 1] == 'X')) {
					res = 10 + res;
				}
				else {
					if ((s[y] == 'L') & (s[y + 1] == 'L')) {
						res = 50 + res;
					}
					else {
						if ((s[y] == 'L') & (s[y + 1] == 'C')) {
							res = 100 - res;
						}
						else {
							if ((s[y] == 'L') & (s[y + 1] == 'D')) {
								res = 500 - res;
							}
							else {
								if ((s[y] == 'L') & (s[y + 1] == 'M')) {
									res = 1000 - res;
								}
							}
						}
					}
				}
			}
		}

		//------------------------------------------------------------Для cта(C)
		if ((s[y] == 'C') & (s[y + 1] == 'I')) {
			res = res + 1;
		}
		else {
			if ((s[y] == 'C') & (s[y + 1] == 'V')) {
				res = 5 + res;
			}
			else {
				if ((s[y] == 'C') & (s[y + 1] == 'X')) {
					res = 10 + res;
				}
				else {
					if ((s[y] == 'C') & (s[y + 1] == 'L')) {
						res = 50 - res;
					}
					else {
						if ((s[y] == 'C') & (s[y + 1] == 'C')) {
							res = 100 + res;
						}
						else {
							if ((s[y] == 'C') & (s[y + 1] == 'D')) {
								res = 500 - res;
							}
							else {
								if ((s[y] == 'C') & (s[y + 1] == 'M')) {
									res = 1000 - res;
								}
							}
						}
					}
				}
			}
		}

		//------------------------------------------------------------Для пятиста(D)
		if ((s[y] == 'D') & (s[y + 1] == 'I')) {
			res = res + 1;
		}
		else {
			if ((s[y] == 'D') & (s[y + 1] == 'V')) {
				res = 5 + res;
			}
			else {
				if ((s[y] == 'D') & (s[y + 1] == 'X')) {
					res = 10 + res;
				}
				else {
					if ((s[y] == 'D') & (s[y + 1] == 'L')) {
						res = 50 - res;
					}
					else {
						if ((s[y] == 'D') & (s[y + 1] == 'C')) {
							res = 100 - res;
						}
						else {
							if ((s[y] == 'D') & (s[y + 1] == 'D')) {
								res = 500 + res;
							}
							else {
								if ((s[y] == 'D') & (s[y + 1] == 'M')) {
									res = 1000 - res;
								}
							}
						}
					}
				}
			}
		}

		//------------------------------------------------------------Для тысячи(M)
		if ((s[y] == 'M') & (s[y + 1] == 'I')) {
			res = res + 1;
		}
		else {
			if ((s[y] == 'M') & (s[y + 1] == 'V')) {
				res = 5 + res;
			}
			else {
				if ((s[y] == 'M') & (s[y + 1] == 'X')) {
					res = 10 + res;
				}
				else {
					if ((s[y] == 'M') & (s[y + 1] == 'L')) {
						res = 50 - res;
					}
					else {
						if ((s[y] == 'M') & (s[y + 1] == 'C')) {
							res = 100 - res;
						}
						else {
							if ((s[y] == 'M') & (s[y + 1] == 'D')) {
								res = 500 - res;
							}
							else {
								if ((s[y] == 'M') & (s[y + 1] == 'M')) {
									res = 1000 + res;
								}
							}
						}
					}
				}
			}
		}
	}

	cout << res << endl;

	return 0;
}
int var1() {
	setlocale(LC_ALL, "Russian");
	int n, m, i, c, s;
	m = 37;
	i = 3;
	c = 64;
	cout << "Введите количество чисел" << endl;
	cin >> n;
	s = (rand() * 13 - 65) % 2;
	for (int k = 1; k <= n; k++)
	{
		s = (m * s + i) % c;
		cout << s << endl;
	}
	return 0;
}
